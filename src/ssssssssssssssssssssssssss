  private async getTicketData(ticketId: number): Promise<FreshDeskTicket | null> {
    const magentoId = extractMagentoId(ticketId);

    try {
      const response = await axios.get(
        `${process.env.ERP_URL}/api/v1/freshdesk/rma/info?magentoId=${magentoId}`,
        {
          headers: {
            Authorization: `Bearer ${process.env.ERP_TOKEN}`,
          },
        }
      );
      return response.data;
    } catch (error) {
      console.error(
        `‚ùå Erro ao buscar RMA para pedido ${magentoId}:`,
        error instanceof Error ? error.message : String(error)
      );
      return null;
    }
  }
